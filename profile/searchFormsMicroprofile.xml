<?xml version="1.0" encoding="UTF-8"?><profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="profile.xsd" useForConfiguration="1" base="base_en_AU" infoUrl="https://github.com/rwahs/providence">
  <profileName>RWAHS - Royal Western Australian Historical Society</profileName>
  <profileDescription>Royal Western Australian Historical Society profile</profileDescription>
  <locales>
    <locale lang="en" country="AU">English (Australian)</locale>
  </locales>
  <lists/>
  <elementSets/>
  <userInterfaces/>
  <relationshipTypes/>
  <roles/>
  <groups/>
  <displays>
    <display code="library_search_display" type="ca_objects" system="1">
      <labels>
        <label locale="en_AU">
          <name>Library Search display</name>
        </label>
      </labels>
      <groupAccess>
        <permission group="admin" access="edit"/>
        <permission group="library" access="read"/>
        <permission group="museum" access="read"/>
        <permission group="photograph" access="read"/>
        <permission group="memorial" access="read"/>
      </groupAccess>
      <bundlePlacements>
        <placement code="preferred_labels">
          <bundle>ca_object_labels.name</bundle>
          <settings>
            <setting name="label" locale="en_AU">Title</setting>
          </settings>
        </placement>
        <placement code="Author">
          <bundle>ca_objects.Author</bundle>
          <settings>
            <setting name="label" locale="en_AU">Name of author</setting>
          </settings>
        </placement>
        <placement code="Publisher">
          <bundle>ca_objects.Publisher</bundle>
          <settings>
            <setting name="label" locale="en_AU">Publisher</setting>
          </settings>
        </placement>
        <placement code="DateOfPublication">
          <bundle>ca_objects.DateOfPublication</bundle>
          <settings>
            <setting name="label" locale="en_AU">Date of publication</setting>
          </settings>
        </placement>
        <placement code="PublicationType">
          <bundle>ca_objects.PublicationType</bundle>
          <settings>
            <setting name="label" locale="en_AU">Type of publication</setting>
          </settings>
        </placement>
        <placement code="idno">
          <bundle>ca_objects.idno</bundle>
          <settings>
            <setting name="label" locale="en_AU">Accession no.</setting>
          </settings>
        </placement>
        <placement code="LibraryNumber">
          <bundle>ca_objects.LibraryNumber</bundle>
          <settings>
            <setting name="label" locale="en_AU">Library no.</setting>
          </settings>
        </placement>
      </bundlePlacements>
    </display>
    <display code="museum_object_search" type="ca_objects" system="1">
      <labels>
        <label locale="en_AU">
          <name>Museum Object search</name>
        </label>
      </labels>
      <bundlePlacements>
        <placement code="preferred_labels">
          <bundle>ca_object_labels.name</bundle>
          <settings>
            <setting name="label" locale="en_AU">Item Name</setting>
          </settings>
        </placement>
        <placement code="ItemDates">
          <bundle>ca_objects.ItemDates</bundle>
        </placement>
        <placement code="Description">
          <bundle>ca_objects.Description</bundle>
        </placement>
        <placement code="MakersMarks">
          <bundle>ca_objects.MakersMarks</bundle>
        </placement>
        <placement code="HistoricalDetails">
          <bundle>ca_objects.HistoricalDetails</bundle>
        </placement>
        <placement code="Donor">
          <bundle>ca_entities</bundle>
          <settings>
            <setting name="label" locale="en_AU">Donor</setting>
            <setting name="restrict_to_relationship_types">donor</setting>
          </settings>
        </placement>
        <placement code="Location">
          <bundle>ca_objects.ca_objects_location</bundle>
          <settings>
            <setting name="label" locale="en_AU">Location</setting>
          </settings>
        </placement>
        <placement code="Thumbnail">
          <bundle>ca_object_representations.media.thumbnail</bundle>
          <settings>
            <setting name="label" locale="en_AU">Thumbnail</setting>
          </settings>
        </placement>
        <placement code="idno">
          <bundle>ca_objects.idno</bundle>
          <settings>
            <setting name="label" locale="en_AU">Accession number</setting>
          </settings>
        </placement>
      </bundlePlacements>
    </display>
  </displays>
